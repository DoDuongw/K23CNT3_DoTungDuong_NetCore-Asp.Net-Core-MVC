@model DtdLesson05.Models.DataModels.DtdMember

@{
    ViewData["Title"] = "Thông tin thành viên";
}

<div class="alert alert-info">
    <h1>@ViewData["Title"]</h1>
    <hr />
    <p><strong>Member ID:</strong> @Model.DtdMembersId</p>
    <p><strong>User Name:</strong> @Model.DtdUserName</p>
    <p><strong>Password:</strong> @Model.DtdPassword</p> 
    <p><strong>Full Name:</strong> @Model.DtdFullName</p>
    <p><strong>Email:</strong> @Model.DtdEmail</p>
</div>

